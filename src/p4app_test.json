{
    "program": "p4src/switch.p4",
    "switch": "simple_switch",
    "compiler": "p4c",
    "options": "--target bmv2 --arch v1model --std p4-16",
    "switch_cli": "simple_switch_CLI",
    "cli": true,
    "pcap_dump": true,
    "enable_log": true,
    "exec_scripts": [
      {
        "cmd": "sudo python controller/coordinator.py &",
        "reboot_run": true
      },
      {
        "cmd": "sudo python controller/l2_controller.py --n s1 --e 1 --t 10 &",
        "reboot_run": true
      },
      {
        "cmd": "sudo python controller/l2_controller.py --n s2 --e 1 --t 10 &",
        "reboot_run": true
      },
      {

        "cmd": "sudo python controller/l2_controller.py --n s3 --e 1 --t 10 &",
        "reboot_run": true
      },
      {
        "cmd": "sudo python controller/l2_controller.py --n s4 --e 1 --t 10 &",
        "reboot_run": true
      }
    ],
    "topo_module": {
      "file_path": "",
      "module_name": "p4utils.mininetlib.apptopo",
      "object_name": "AppTopoStrategies"
    },
    "controller_module": null,
    "topodb_module": {
      "file_path": "",
      "module_name": "p4utils.utils.topology",
      "object_name": "Topology"
    },
    "mininet_module": {
      "file_path": "",
      "module_name": "p4utils.mininetlib.p4net",
      "object_name": "P4Mininet"
    },
    "topology": {
      "assignment_strategy": "l2",
      "auto_arp_tables": false,
      "links": [
        ["h1", "s1"], ["h1", "s2"], ["h1", "s3"], ["h1", "s4"],
        ["h2", "s1"], ["h2", "s2"], ["h2", "s3"], ["h2", "s4"]
      ],
      "hosts": {
        "h1": {
        },
        "h2": {
        }
      },
      "switches": {
        "s1": {
        },
        "s2": {
        },
        "s3": {
        },
        "s4": {
        }
      }
    }
  }
  